			".ENABLED": false,
			".URL": "https://ds.widedigital.com.br/datetime.php",
			".SSL": true
		}
	},

	"AUTOMATION": {
		".ENABLED": true,
		".TIMER": {
			".ENABLED": false,
			".TIMER": 0.50
		},
		".SHOW": true,
		".KEYS": [
			"f5",
			"esc",
			"enter"
		]
	},

	"DEV": {
		".ENABLED": false,
		".KEYS": "",
		".URL": ""
	}
}

